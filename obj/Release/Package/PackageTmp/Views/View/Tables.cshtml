@{
    ViewBag.Title = "Admin";
}

@section scripts {
    <link href="~/Styles/ezCrud.css" rel="stylesheet" />
    <link href="~/Styles/ezRefs.css" rel="stylesheet" />
    <script src="~/Scripts/ezCrud.js"></script>
    <script src="~/Scripts/ezRefs.js"></script>
    <script src="~/Scripts/Tables.js"></script>
    <script>
    </script>
}

<table>
    <tr>
        <td class="tdLeftMenuArea">
            <ul>
                <li id="adminUsers">User Roles</li>
                <li id="adminCustomers">Customers</li>
                <li id="adminProperties">Properties</li>
                <li id="adminmenuDropDowns">Dropdown Values</li>
                <li id="adminmenuOptions">Options</li>
            </ul>
        </td>
        <td class="adminWorkArea">
            <div class="adminWorkAreaDiv" id="divUsers">
                <div class="sectionHeader">Assign User Roles</div>
                <table style="width:100%">
                    <tr>
                        <td><div class="augBox" id="divUserList"></div></td>
                        <td>
                            <div class="augBox" id="userDetails">
                                <div class="crudArea" id="divUserDetails" style="min-width:444px;">
                                    <table>
                                        <tr>
                                            <td colspan="2">
                                                User:
                                                <span id="txtSelectedUser" data-bind="text:FullName"></span>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td>User:</td>
                                            <td><input type="radio" name="radioRoleGroup" value="1" data-bind="checked:UserRole" /></td>
                                        </tr>
                                        <tr>
                                            <td>Key User:</td>
                                            <td><input type="radio" name="radioRoleGroup" value="2" data-bind="checked:UserRole" /></td>
                                        </tr>
                                        <tr>
                                            <td>Admin:</td>
                                            <td><input type="radio" name="radioRoleGroup" value="3" data-bind="checked:UserRole" /></td>
                                        </tr>
                                        <tr>
                                            <td colspan="2">
                                                <table class="tbLRbox">
                                                    <tr>
                                                        <td>Business Unit</td>
                                                        <td></td>
                                                        <td>Assigned</td>
                                                    </tr>
                                                    <tr>
                                                        <td><div class="tdLRchoiceBox" id="leftdivLRbox"></div></td>
                                                        <td id="tdLRCenterBox">
                                                            <div id="btnAddAllBizUnits"> >> </div>
                                                            <div id="btnAddBizUnit"> &nbsp; > </div>
                                                            <div id="btnRemoveBizUnit"> &nbsp; < </div>
                                                            <div id="btnRemoveAllBizUnits"> << </div>
                                                        </td>
                                                        <td><div class="tdLRchoiceBox" id="rightdivLRbox"></div></td>
                                                    </tr>
                                                </table>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td colspan="2"><span class="linky" id="btnAddNewUser">Add New User</span></td>
                                        </tr>
                                        <tr>
                                            <td colspan="2"><span class="flatButton" id="btnUpdateUserRole">Update User Role</span></td>
                                        </tr>
                                    </table>
                                    @*<span class="flatButton pinToBottom50" id="btnUpdateUserRole">Update User Role</span>
                                        <span class="linky pinToBottom75" id="btnAddNewUser">Add New User</span>*@
                                </div>
                            </div>
                        </td>
                    </tr>
                </table>
            </div>
            <div class="adminWorkAreaDiv" id="divCustomers">
                <div class="sectionHeader">Customers</div>
                <div id="divCustomerEdit" class="adminEditTable">
                    <table>
                        <tr><td>Customer Name</td>
                            <td colspan="5"><input class="roundedInput" data-bind="value:CustomerName" /></td>
                        </tr>
                        <tr><td>Street Address</td>
                            <td colspan="5"><input class="roundedInput" data-bind="value:BillingAddressStreet" /></td>
                        </tr>
                        <tr><td>City</td>
                            <td><input class="roundedInput" data-bind="value:BillingAddressCity" /></td>
                            <td>State</td>
                            <td>
                                <select class="roundedInput" data-bind="value:BillingAddressState">
                                    <option value="00">select state</option>
                                    <option value="AL">Alabama</option>
                                    <option value="AK">Alaska</option>
                                    <option value="AZ">Arizona</option>
                                    <option value="AR">Arkansas</option>
