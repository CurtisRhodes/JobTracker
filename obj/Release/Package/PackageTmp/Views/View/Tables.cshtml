                </div>
                <div class="divAdminListTable" id="divPropertyList"></div>
            </div>
            <div class="adminWorkAreaDiv" id="divDropDowns">
                <div class="sectionHeader">Dropdown List Values</div>
                <div id="divRefEdit">
                    <select id="selRefTypes"></select>
                    <div id="divRefItems"></div>
                    <input id="txtRefItem" />
                    <div>
                        <button id="btnAddSaveRef">Add</button>
